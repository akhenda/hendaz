{
  "extends": ["../../.eslintrc.json", "@hendaz/eslint-config/basic"],
  "ignorePatterns": ["!**/*", "jest.config.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
